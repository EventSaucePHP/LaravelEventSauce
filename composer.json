{
    "name": "eventsauce/laravel-eventsauce",
    "description": "Integration support of EventSauce for the Laravel framework",
    "keywords": ["laravel", "event sourcing", "eventsauce"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/EventSaucePHP/LaravelEventSauce/issues",
        "source": "https://github.com/EventSaucePHP/LaravelEventSauce"
    },
    "authors": [
        {
            "name": "Dries Vints",
            "email": "dries.vints@gmail.com"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        }
    ],
    "require": {
        "php": "^7.2",
        "eventsauce/eventsauce": "^0.5.0",
        "illuminate/bus": "^5.7",
        "illuminate/container": "^5.7",
        "illuminate/queue": "^5.7",
        "illuminate/support": "^5.7",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "phpstan/phpstan": "@stable",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
          "EventSauce\\LaravelEventSauce\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EventSauce\\LaravelEventSauce\\EventSauceServiceProvider"
            ]
        }
    }
}
